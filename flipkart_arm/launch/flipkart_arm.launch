<launch>

 <!-- Launch Your robot arms launch file which loads the robot in Gazebo and spawns the
controllers -->
 <include file = "$(find test_description)/launch/arm_urdf.launch" /> <!-- Launch Moveit Move Group Node -->
 <include file = "$(find flipkart_arm)/launch/move_group.launch" />
 <!-- Run Rviz and load the default configuration to see the state of the move_group node -->
 <arg name="use_rviz" default="true" />
 <include file="$(find flipkart_arm)/launch/moveit_rviz.launch" if="$(arg
use_rviz)">
 <arg name="rviz_config" value="$(find flipkart_arm)/launch/moveit.rviz"/> </include>
</launch>
