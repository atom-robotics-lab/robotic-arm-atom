<?xml version="1.0" ?>
<sdf version="1.6">

    <!-- Import the trolley model -->
  <model name="trolley_base">
    <link name="link">
      <inertial>
        <mass>10</mass>
        <inertia>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyz>0</iyz>
            <ixx>0.05288</ixx>
            <iyy>0.05288</iyy>
            <izz>0.09132</izz>
          </inertia>
      </inertial>
      <collision name="collision">
        <geometry>
          <mesh>
            <uri>model://trolley_base/meshes/trolley_base.dae</uri>
          </mesh>
        </geometry>
      </collision>
      <visual name="visual">
	    <geometry>
          <mesh>
            <uri>model://trolley_base/meshes/trolley_base.dae</uri>
            <scale>1 1 1</scale>
          </mesh>
        </geometry>
      <meta> <layer> 1 </layer></meta>
     </visual>
    </link>
  </model>
  
  
</sdf>
