<node name="controller_spawner" pkg="controller_manager" type="spawner" respawn="false" output="screen" ns="nag_raj" args="Revolute 1_position_controller Revolute 2_position_controller Revolute 3_position_controller Revolute 4_position_controller Revolute 5_position_controller joint_state_controller "/>
<node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher" respawn="false" output="screen">
  <remap from="/joint_states" to="/nag_raj/joint_states"/>
</node>

</launch>